{
    "name": "cmarsiglia/tenant-core",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Carlos Marsiglia",
            "email": "carlos.marsiglia@gmail.com"
        }
    ],
    
    "require": {
      "php": "7.*",
      "phpseclib/phpseclib": "~2.0",
      "doctrine/dbal": "~2.5",
      "ramsey/uuid": "^3.5",
      "laravel/framework": "5.6.*"
    },
    "require-dev": {
      "phpunit/phpunit": "~7.0",
      "symfony/dom-crawler": "~3.1",
      "laravel/laravel": "5.6.*",
      "mockery/mockery": "^0.9.9"
    },
    "autoload": {
        "psr-4": {
            "Cmarsiglia\\TenantCore\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cmarsiglia\\TenantCore\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cmarsiglia\\TenantCore\\Providers\\TenantProvider"

            ],
            "aliases": {
                "TenantCore": "Cmarsiglia\\TenantCore\\Facades\\TenantFacade"
            }
        }
    }

}
